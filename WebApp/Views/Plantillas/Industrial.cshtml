                /*
        Estilos Home (Publicitario)
        */
                .navegador {
                    margin: 0%;
                    padding: 0%;
                }

                .borde {
                    border-bottom: 2px #000 solid;
                    margin-bottom: 30px;
                    height: 78px;
                }

                .carrusel {
                    width: 90%;
                }

                .bg-nav-home {
                    background: rgba(0, 0, 0, 0.5);
                }

                .font-brush {
                    font-family: "Brush Script Std", Arial, Helvetica, sans-serif;
                }

                .br-nav {
                    border-bottom: 2.5px solid #000033 !important;
                }

                .item-efec {
                    overflow: hidden;
                    position: relative;
                    border-bottom: 4px solid #ebfbfc;
                }

                    .item-efec:hover {
                        border-bottom-color: #000033;
                    }

                .brillo {
                    display: none;
                    background: rgba(45, 166, 190, 0.7);
                    position: absolute;
                    width: 50px;
                    height: 150px;
                    transform: rotateZ(15deg);
                    top: -10px;
                    left: -40px;
                }

                .item-efec:hover > .brillo {
                    animation: rayo .7s ease-in-out;
                    display: block;
                }

                @@keyframes rayo {
                    0% {
                        top: -10px;
                        left: -150px;
                    }

                    50% {
                        top: -10px;
                        left: 130px;
                    }

                    100% {
                        top: -10px;
                        left: -35px;
                    }
                }

                .box-tabs {
                    padding: 0rem !important;
                }

                .menu-titulo {
                    text-align: center;
                }

                .p-hov {
                    display: block;
                    overflow: hidden;
                    transition: all 0.8s ease;
                }

                    .p-hov:hover {
                        transform: scale(1.2)
                    }

                .nav-item {
                    padding: 10px;
                    color: black !important;
                }

                .noso {
                    background-image: url("../img/Industrial/noso.jpg");
                }

                .caja {
                    background: rgba(221, 221, 221, 0.8);
                }
                /* footer */
                footer {
                    background-image: linear-gradient(to right,#9ceef5, #ebfbfc);
                    position: unset;
                }

